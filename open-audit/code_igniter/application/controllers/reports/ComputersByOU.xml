<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Computers by OU</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT sys_sw_windows.windows_id, sys_sw_windows.windows_active_directory_ou, count(system.system_id) as count FROM system LEFT JOIN sys_sw_windows ON (system.system_id = sys_sw_windows.system_id AND system.timestamp = sys_sw_windows.timestamp) LEFT JOIN oa_group_sys ON system.system_id = oa_group_sys.system_id WHERE oa_group_sys.group_id = @group AND man_status = 'production' GROUP BY sys_sw_windows.windows_active_directory_ou]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
	</details>
	<columns>
		<column>
			<column_order>0</column_order>
			<column_name>Organizational Unit</column_name>
			<column_variable>windows_active_directory_ou</column_variable>
			<column_type>link</column_type>
			<column_link>/report/specific_ou/$group_id/</column_link>
			<column_secondary>windows_id</column_secondary>
			<column_ternary></column_ternary>
			<column_align>left</column_align>
		</column>
		<column>
			<column_order>1</column_order>
			<column_name>Count</column_name>
			<column_variable>count</column_variable>
			<column_type>text</column_type>
			<column_link></column_link>
			<column_secondary></column_secondary>
			<column_ternary></column_ternary>
			<column_align>left</column_align>
		</column>
	</columns>
</report>