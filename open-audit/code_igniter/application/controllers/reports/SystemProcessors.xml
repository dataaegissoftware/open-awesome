<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>System Processors</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, system.man_ip_address, CAST(system.pc_memory/1024 AS UNSIGNED) AS memory, sys_hw_processor.processor_cores, sys_hw_processor.processor_speed, sys_hw_processor.processor_description FROM system LEFT JOIN sys_hw_processor ON (system.system_id = sys_hw_processor.system_id AND system.timestamp = sys_hw_processor.timestamp) LEFT JOIN oa_group_sys ON system.system_id = oa_group_sys.system_id WHERE oa_group_sys.group_id = @group AND processor_description IS NOT NULL GROUP BY system.hostname ORDER BY system.hostname]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
