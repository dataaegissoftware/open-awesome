<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Specific OU</report_name>
		<report_display_in_menu>n</report_display_in_menu>
		<report_sql><![CDATA[SELECT man_icon, hostname, system.system_id as system_id, man_model, man_owner, org_name, location_name FROM system LEFT JOIN oa_group_sys ON system.system_id = oa_group_sys.system_id LEFT JOIN sys_sw_windows ON (system.system_id = sys_sw_windows.system_id AND system.timestamp = sys_sw_windows.timestamp) LEFT JOIN oa_org ON system.man_org_id = oa_org.org_id LEFT JOIN oa_location ON system.man_location_id = oa_location.location_id WHERE oa_group_sys.group_id = @group AND sys_sw_windows.windows_active_directory_ou = (SELECT windows_active_directory_ou FROM sys_sw_windows WHERE windows_id = ?)]]></report_sql>
		<report_view_file>v_report</report_view_file>
